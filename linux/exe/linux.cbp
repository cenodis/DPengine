<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="launcher (linux)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="src/;src/exec/;" />
		<Build>
			<Target title="Debug">
				<Option output="../linux-gtk" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".." />
				<Option object_output=".obj/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="../linux-gtk" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".." />
				<Option object_output=".obj/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fexpensive-optimizations" />
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Linker>
			<Add library="engine" />
			<Add directory=".." />
		</Linker>
		<Unit filename="../../core/engine.h">
			<Option virtualFolder="src/" />
		</Unit>
		<Unit filename="../../exec/exec.c">
			<Option compilerVar="CC" />
			<Option virtualFolder="src/exec/" />
		</Unit>
		<Unit filename="../../exec/exec.h">
			<Option virtualFolder="src/exec/" />
		</Unit>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
			<Option virtualFolder="src/" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
