<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="launcher (win32)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="src\;src\rsrc\;src\exec\;" />
		<Build>
			<Target title="Debug">
				<Option output="..\win32" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".." />
				<Option object_output=".obj\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="..\win32" prefix_auto="1" extension_auto="1" />
				<Option working_dir=".." />
				<Option object_output=".obj\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-fexpensive-optimizations" />
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Linker>
			<Add library="comctl32" />
			<Add library="engine" />
			<Add directory=".." />
		</Linker>
		<Unit filename="..\..\core\engine.h">
			<Option virtualFolder="src\" />
		</Unit>
		<Unit filename="..\..\exec\exec.c">
			<Option compilerVar="CC" />
			<Option virtualFolder="src\exec\" />
		</Unit>
		<Unit filename="..\..\exec\exec.h">
			<Option virtualFolder="src\exec\" />
		</Unit>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
			<Option virtualFolder="src\" />
		</Unit>
		<Unit filename="rsrc\rsrc.h">
			<Option virtualFolder="src\rsrc\" />
		</Unit>
		<Unit filename="rsrc\rsrc.rc">
			<Option compilerVar="WINDRES" />
			<Option virtualFolder="src\rsrc\" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
